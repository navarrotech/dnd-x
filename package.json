{
  "name": "dnd-x",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "proto": "pbjs -t static-module --es6 --keep-case -w es6 -o ./src/modules/protobuf/schema.js ../schema.proto && pbts -o ./src/modules/protobuf/schema.d.ts ./src/modules/protobuf/schema.js",
    "deploy:production": "firebase deploy",
    "deploy:staging": "firebase hosting:channel:deploy staging",
    "test": "vitest run --dom --coverage",
    "test:dev": "DEBUG_PRINT_LIMIT=100000 vites watch --ui --dom --coverage.enabled=true",
    "analyze": "vite-bundle-analyzer"
  },
  "dependencies": {
    "@creativebulma/bulma-tooltip": "^1.2.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
