    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-i18next": "^6.0.9",
