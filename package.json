    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "bulma": "^1.0.1",
    "bulma-divider": "^0.2.0",
    "country-flag-icons": "^1.5.12",
    "eslint-plugin-import": "^2.29.1",
    "events": "^3.3.0",
    "firebase": "^10.13.0",
    "happy-dom": "^14.12.3",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "pdf-lib": "^1.17.1",
    "protobufjs": "^7.3.0",
    "rc-dock": "^3.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "react-redux": "^9.1.2",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "redux": "^5.0.1",
    "sass": "^1.77.4",
    "socket.io-client": "^4.7.5",
    "vitest": "^1.6.0",
    "yup": "^1.4.0"
